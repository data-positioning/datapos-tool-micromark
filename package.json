{
    "name": "@datapos/datapos-tool-micromark",
    "version": "0.1.987",
    "description": "A library that wraps the Micromark markdown parser and Speed Highlight code highlighter.",
    "license": "MIT",
    "private": false,
    "type": "module",
    "homepage": "https://www.datapos.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/data-positioning/datapos-tool-micromark.git"
    },
    "bugs": {
        "url": "https://github.com/data-positioning/datapos-tool-micromark/issues"
    },
    "module": "./dist/datapos-tool-micromark.es.js",
    "types": "./dist/types/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/datapos-tool-micromark.es.js",
            "types": "./dist/types/src/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@speed-highlight/core": "^1.2.12",
        "micromark": "^4.0.2",
        "micromark-extension-gfm-table": "^2.1.1"
    },
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.133",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "jiti": "^2.6.1",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "license-report-recursive": "^6.8.2",
        "nanoid": "^5.1.6",
        "npm-check-updates": "^19.1.2",
        "owasp-dependency-check": "^1.0.0",
        "prettier": "^3.7.1",
        "rollup-plugin-visualizer": "^6.0.5",
        "typescript": "^5.9.3",
        "vite": "^7.2.4",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "npm audit",
        "build": "vite build",
        "check:update": "npm outdated; npm-check-updates -i",
        "check:vulnerabilities": "npm run _check:owaspDependencyCheck; npm run _check:owaspBageInsertiions",
        "document": "npm run _document:licenceReportJSON && npm run _document:licenceReportMarkdown && npm run _document:licenceReportCheck && npm run _document:insertLicensesIntoReadme && npm run _document:licenceTree && npm run _document:licenceTreeCheck",
        "format": "prettier --write src/",
        "lint": "eslint .",
        "publish:toNPM": "npm publish --access public",
        "release": "npm run _bump:version && npm run _build:config && npm run build && npm run _sync:withGitHub && npm run _upload && npm run publish:toNPM",
        "sync": "npm run _bump:version && npm run _sync:withGitHub",
        "test": "node -e \"import('@datapos/datapos-development').then(m => m.echoScriptNotImplemented('Test'))\"",
        "update:dataPosDeps": "npm run _update:developDep",
        "_bump:version": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "_check:owaspDependencyCheck": "set -a && source .env && set +a && owasp-dependency-check --project \"@datapos/datapos-tool-micromark\" --nvdApiKey \"$NVD_API_KEY\" --nodePackageSkipDevDependencies --enableRetired",
        "_check:owaspBageInsertiions": "node -e \"import('@datapos/datapos-development').then(m => m.insertOWASPDependencyCheckBadgeIntoReadme())\"",
        "_build:config": "node -e \"import('@datapos/datapos-development').then(m => m.buildConfig())\"",
        "_document:licenceReportJSON": "license-report --only=prod,peer --department.value=n/a --licensePeriod=n/a --material=n/a --relatedTo.value=n/a > licenses.json",
        "_document:licenceReportMarkdown": "license-report --config license-report-config.json --only=prod,peer --output=markdown > licenses.md",
        "_document:licenceReportCheck": "license-report-check --source ./licenses.json --allowed 'MIT' --allowed 'n/a' --allowed 'Apache-2.0' --allowed 'CC0-1.0' --output=table",
        "_document:licenceTree": "license-report-recursive --only=prod,peer --recurse --output=tree > licenseTree.json",
        "_document:licenceTreeCheck": "license-report-check --source ./licenseTree.json --allowed 'MIT' --allowed 'n/a' --allowed 'Apache-2.0' --allowed 'CC0-1.0' --output=table",
        "_document:insertLicensesIntoReadme": "node -e \"import('@datapos/datapos-development').then(m => m.insertLicensesIntoReadme())\"",
        "_sync:withGitHub": "node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "_update:developDep": "npm install --save-dev @datapos/datapos-development@latest",
        "_upload": "npm run _upload:moduleToR2 && npm run _upload:moduleConfigToDO",
        "_upload:moduleConfigToDO": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleConfigToDO())\"",
        "_upload:moduleToR2": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleToR2('datapos-engine-eu/tools/micromark'))\""
    }
}